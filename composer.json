{
    "name": "robots-dreams/project-template",
    "description": "This template contains several code quality tools to easily setup a new project.",
    "type": "project",
    "license": "MIT License",
    "keywords": [
        "code-quality",
        "template",
        "php",
        "project"
    ],
    "homepage": "https://www.robots-dreams.net",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src\\App\\"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "tests/AppTest"
        }
    },
    "require-dev": {
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10.2",
        "squizlabs/php_codesniffer": "^3.7",
        "captainhook/captainhook": "^5.16",
        "rector/rector": "^0.18.0"
    }
}
